{
  "manifest_version": 2,

  "name": "CS: GO scores",
  "description": "Get the scores to the most recent pro matches!",
  "version": "1.7",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "content_scripts": [{
    "matches": ["http://*/*", "https://*/*"],
    "js": ["./popup.js"],
    "run_at": "document_start"

    }],

  "permissions": [
    "tabs",
    "activeTab",
    "https://ajax.googleapis.com/"

  ]
}